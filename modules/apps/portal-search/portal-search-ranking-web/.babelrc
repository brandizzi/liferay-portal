{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"node": "current"
				}
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-proposal-object-rest-spread",
		[
			"module-resolver",
			{
				"root": ["./src/main/resources/META-INF/resources/js/"],
				"alias": {
					"test": "./test/js/"
				}
			}
		]
	]
}